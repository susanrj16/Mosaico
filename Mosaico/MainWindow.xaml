<Window x:Class="Mosaico.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mosaico"
        mc:Ignorable="d"
        Title="Mosaico" Height="450" Width="600" MinHeight="500" MinWidth="450" Icon="32.ico">
    <DockPanel>
        <TextBlock DockPanel.Dock="Top" Height="100" Background="Purple"/>
        <TextBlock DockPanel.Dock="Left" Width="100" Background="Purple"/>
        <TextBlock DockPanel.Dock="Right" Width="100" Background="Green"/>
        <TextBlock DockPanel.Dock="Bottom" Height="100" Background="Green"/>
        <TextBlock DockPanel.Dock="Bottom" Height="100" Background="Red"/>
        <TextBlock DockPanel.Dock="Left" Width="100" Background="Red"/>
        <TextBlock DockPanel.Dock="Top" Height="100" Background="HotPink"/>
        <TextBlock DockPanel.Dock="Right" Width="100" Background="HotPink"/>
        <TextBlock Background="Black"/>

    </DockPanel>
</Window>
